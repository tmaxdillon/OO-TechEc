function [] = visOoSsmOut(par,reso,S)

addpath(genpath('~/Dropbox (MREL)/MATLAB/Helper'))

if isequal(reso,'wiod')
    tr = 'Wind (Opt. Durability)';
elseif isequal(reso,'wico')
    tr = 'Wind (Conservative)';
elseif isequal(reso,'inau')
    tr = 'Solar (Automated Clean)';
elseif isequal(reso,'inhu')
    tr = 'Solar (Human Clean)';
elseif isequal(reso,'wcon')
    tr = 'Wave (Conservative)';
elseif isequal(reso,'woco')
    tr = 'Wave (Opt. Cost)';
elseif isequal(reso,'wodu')
    tr = 'Wave (Opt. Durability)';
elseif isequal(reso,'dgen')
    tr = 'Diesel';
end

%set parameter specific settings
    %wind
if isequal(opt.tuned_parameter,'tiv')
elseif isequal(opt.tuned_parameter,'tcm')
    xl = 'Turbine Cost Multiplier';
elseif isequal(opt.tuned_parameter,'twf')
    xl = 'Turbine Weight Factor [kg/kW]';
elseif isequal(opt.tuned_parameter,'cis')   
    xl = 'Cut In Speed [m/s]';
elseif isequal(opt.tuned_parameter,'rsp')
    xl = 'Rated Speed [m/s]';
elseif isequal(opt.tuned_parameter,'cos')
    xl = 'Cut Out Speed [m/s]';
elseif isequal(opt.tuned_parameter,'tef')
    xl = 'Turbine Efficiency';
elseif isequal(opt.tuned_parameter,'szo')
    xl = 'Surface Roughness [mm]';
    %inso
elseif isequal(opt.tuned_parameter,'pvd')
    xl = 'Panel Degradation [%/year]';
elseif isequal(opt.tuned_parameter,'pcm')
    xl = 'PV System Cost Multiplier';
elseif isequal(opt.tuned_parameter,'pwf')
    xl = 'Panel Weight Factor [kg/m^3]';
elseif isequal(opt.tuned_parameter,'pve')
    xl = 'PV Efficiency';
    %wave
elseif isequal(opt.tuned_parameter,'wiv')
    xl = 'WEC Interventions';
elseif isequal(opt.tuned_parameter,'wcm')
    xl = 'WEC Cost Multiplier';
elseif isequal(opt.tuned_parameter,'whl')
    xl = 'WEC Hotel Load [% of Gr]';
elseif isequal(opt.tuned_parameter,'ect')
    xl = 'Electrical Efficiency';
    %dies
elseif isequal(opt.tuned_parameter,'giv')
    xl = 'Generator Interventions';
elseif isequal(opt.tuned_parameter,'fco')
    xl = 'Fuel Cost [$/L]';
elseif isequal(opt.tuned_parameter,'fca')
    xl = 'Fuel Capacity [L]';
elseif isequal(opt.tuned_parameter,'fsl')
    xl = 'Fuel Shelf Life [mo]';
elseif isequal(opt.tuned_parameter,'oci')
    xl = 'Oil Change Interval [hours]';
elseif isequal(opt.tuned_parameter,'gcm')
    xl = 'Generator Cost Multiplier';
    %all
elseif isequal(opt.tuned_parameter,'lft')
    xl = 'Lifetime [years]';
elseif isequal(opt.tuned_parameter,'dtc')
    xl = 'Distance To Coast [km]';
elseif isequal(opt.tuned_parameter,'osv')
    xl = 'Offshore Support Vessel Cost [$/day]';
elseif isequal(opt.tuned_parameter,'spv')
    xl = 'Specialized Vessel Cost [$/day]';
elseif isequal(opt.tuned_parameter,'tmt')
    xl = 'Time on Site for Maintenance [h]';
elseif isequal(opt.tuned_parameter,'eol')
    xl = 'Battery End of Life [%]';
elseif isequal(opt.tuned_parameter,'dep')
    xl = 'Water Depth [m]';
elseif isequal(opt.tuned_parameter,'bcc')
    xl = 'Battery Cell Cost [$/kWh]';
elseif isequal(opt.tuned_parameter,'bhc')
    xl = 'Battery Housing Cost Multiplier';
elseif isequal(opt.tuned_parameter,'utp')
    xl = 'Uptime Percent [%]';
elseif isequal(opt.tuned_parameter,'ild')
    xl = 'Instrumentation Load [W]';
elseif isequal(opt.tuned_parameter,'sdr')
    xl = 'Self Discharge Rate [%/month]';
end

%unpack data structure
for uc = 1:2
    for l = 1:3
        for i = 1:10
            C(l,uc,i) = S(l,uc).(par)(i).output.min.cost;
        end
        co(l,uc) = S(l,uc).s0.output.min.cost;
    end
end
ta = S(l,uc).(par)(i).opt.tuning_array;

%plot settings
loc = {'argBasin','cosEndurance','irmSea'};

% for a = 1:size(array,1)
%     for r = 1:size(array,2)
%         CapEx(a,r) = array(a,r).output.min.CapEx;
%         OpEx(a,r) = array(a,r).output.min.OpEx;
%     end
%     tp{a} = array(a,1).opt.tuned_parameter;
%     ta(a,:) = array(a,1).opt.tuning_array;
% end
% 
% tc = s0.output.min.cost;

%purple: [.5 0 .5]

figure
set(gcf,'color','w')
ax(1) = subplot(1,2,1);
for l = 1:3
    plot(ta,C(l,1)/co(l,1),'DisplayName',loc{l})
    hold on
end
xlabel(xl)
ylabel('Cost [%]')
set(ax(1),'FontSize',14)
set(ax(1),'LineWidth',1.1)
legend('show','location','southoutside')
title([tr ', ' short-term'])
ax(1) = subplot(1,2,2);
for l = 1:3
    plot(ta,C(l,2)/co(l,2),'DisplayName',loc{l})
    hold on
end
xlabel(xl)
ylabel('Cost [%]')
set(ax(2),'FontSize',14)
set(ax(2),'LineWidth',1.1)
legend('show','location','southoutside')
title([tr ', parameter: ' tp ', long-term'])
% hL = legend('show','location','southoutside','Orientation','horizontal');
% newPosition = [0.41 .03 0.2 0.01];
% newUnits = 'normalized';
% set(hL,'Position', newPosition,'Units', newUnits,'FontSize',16);

end
